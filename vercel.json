{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "client/build",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/client/build/$1"
    }
  ],
  "env": {
    "REACT_APP_API_BASE_URL": "https://806d0f0c7e00.ngrok-free.app"
  }
}